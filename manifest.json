{
    "name": "Repeated.li",
    "version": "0.1",
    "description": "Automate your learning",
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": [
            "jquery-3.3.1.js"
        ]
    }],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage",
        "identity",
        "identity.email",
        "declarativeContent",
        "tabs"
    ],
    "browser_action": {
        "default_title": "Repeated.li",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "chrome_url_overrides": {
        "newtab": "cards.html"
    },
    "manifest_version": 2
}